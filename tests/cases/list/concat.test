# basic: Basic concatenation examples
concat([1, 2]::list<i32>, [3, 4]::list<i32>) = [1, 2, 3, 4]::list<i32>
concat(['a', 'b']::list<string>, ['c']::list<string>) = ['a', 'b', 'c']::list<string>

# empty: Concatenation with empty lists
concat([]::list<i32>, [1, 2]::list<i32>) = [1, 2]::list<i32>
concat([1, 2]::list<i32>, []::list<i32>) = [1, 2]::list<i32>
concat([]::list<i32>, []::list<i32>) = []::list<i32>

# nullable_elements: Lists with null elements
concat([1, Null]::list<i32?>, [2, 3]::list<i32?>) = [1, Null, 2, 3]::list<i32?>
concat([1]::list<i32?>, [Null]::list<i32?>) = [1, Null]::list<i32?>
