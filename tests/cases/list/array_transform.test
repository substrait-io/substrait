# basic: Basic array transformation examples with expressions
array_transform([1, 2, 3]::List<i32>, (x -> x * 2)::lambda<i32 -> i32>) = [2, 4, 6]::List<i32>
array_transform([5, 10, 15]::List<i32>, (x -> x + 1)::lambda<i32 -> i32>) = [6, 11, 16]::List<i32>
array_transform([]::List<i32>, (x -> x * 2)::lambda<i32 -> i32>) = []::List<i32>

# function_calls: Using function call syntax
array_transform([1, 2, 3]::List<i32>, (x -> multiply(x, 2::i32)::i32)::lambda<i32 -> i32>) = [2, 4, 6]::List<i32>

# nullable_elements: Handling nullable elements
array_transform([1, Null, 3]::List<i32?>, (x -> multiply(x, 2::i32)::i32?)::lambda<i32? -> i32?>) = [2, Null, 6]::List<i32?>
