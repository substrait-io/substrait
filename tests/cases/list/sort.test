### SUBSTRAIT_INCLUDE: '/extensions/functions_list.yaml'

# basic: Basic sorting examples
sort([3, 1, 2]::list<i32>) = [1, 2, 3]::list<i32>
sort([5, 2, 8, 1]::list<i32>) = [1, 2, 5, 8]::list<i32>
sort(['c', 'a', 'b']::list<string>) = ['a', 'b', 'c']::list<string>

# empty: Empty list
sort([]::list<i32>) = []::list<i32>

# single_element: Single element list
sort([42]::list<i32>) = [42]::list<i32>

# already_sorted: Already sorted list
sort([1, 2, 3]::list<i32>) = [1, 2, 3]::list<i32>

# nullable_elements: Null elements placed at end
sort([3, Null, 1]::list<i32?>) = [1, 3, Null]::list<i32?>
sort([Null, 2, Null, 1]::list<i32?>) = [1, 2, Null, Null]::list<i32?>
